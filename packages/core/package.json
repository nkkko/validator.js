  "files": [
    "src",
    "cjs",
    "esm",
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "bannerjs": "^3.0.1",
    "rollup": "^4.0.0"
  }
