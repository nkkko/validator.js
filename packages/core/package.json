  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/validator.js"
  },
  "keywords": [
    "validator",
    "validator.tool",
    "validator.js",
    "validation",
    "validate",
    "sanitization",
    "sanitize",
    "sanitisation",
    "sanitise",
    "assert"
